{
  "$schema": "https://raw.githubusercontent.com/QuestPackageManager/QPM.Package/refs/heads/main/qpm.schema.json",
  "version": "0.1.1",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "ImageCoverExpander",
    "id": "imagecoverexpander",
    "version": "0.1.1",
    "url": null,
    "additionalData": {
      "overrideSoName": "libimagecoverexpander.so",
      "cmake": true
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1"
      ],
      "clean": [
        "pwsh ./scripts/build.ps1 -clean"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1 $0:?"
      ],
      "logcat": [
        "pwsh ./scripts/start-logging.ps1"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1",
        "pwsh ./scripts/createqmod.ps1"
      ],
      "restart": [
        "pwsh ./scripts/restart-game.ps1"
      ],
      "stack": [
        "pwsh ./scripts/ndk-stack.ps1"
      ],
      "tomb": [
        "pwsh ./scripts/pull-tombstone.ps1"
      ]
    },
    "ndk": "^27.2.12479018",
    "qmodIncludeDirs": [
      "./build",
      "./extern/libs"
    ],
    "qmodIncludeFiles": [],
    "qmodOutput": "./ImageCoverExpander.qmod"
  },
  "dependencies": [
    {
      "id": "config-utils",
      "versionRange": "^2.0.2",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.6",
      "additionalData": {}
    },
    {
      "id": "paper2_scotland2",
      "versionRange": "^4.5.0",
      "additionalData": {}
    },
    {
      "id": "beatsaber-hook",
      "versionRange": "^6.4.0",
      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "=0.4.49",
      "additionalData": {}
    },
    {
      "id": "kaleb",
      "versionRange": "^0.1.9",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.18.2",
      "additionalData": {}
    }
  ]
}